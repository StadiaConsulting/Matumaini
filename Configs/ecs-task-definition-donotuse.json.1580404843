[
  {
    "family": "kchmatumainiservice",
    "cpu": "256",
    "memory": "512",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
      "FARGATE"
    ],
    "executionRoleArn": "arn:aws:iam::804524942427:role/EcsServiceRole",
    "taskRoleArn": "REPLACE_ME_ECS_TASK_ROLE_ARN",
    "containerDefinitions": [
      {
        "name": "KCHMatumaini-Service",
        "image": "804524942427.dkr.ecr.us-east-1.amazonaws.com/kchmatumaini/service:latest",
        "portMappings": [
        {
          "containerPort": 8080,
          "protocol": "http"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "kchmatumaini-logs",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "awslogs-kchmatumaini-service"
        }
      },
      "essential": true
    }
  ]
}
]
